<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping default-lazy="false" default-access="field" default-cascade="save-update">
    <class name="ar.edu.unq.desapp.grupoa.model.System.Consolidation" table="Consolidation" >
        <id name="id" type="java.lang.Integer">
            <column name="CONS_ID" />
            <generator class="identity" />
        </id>

		<property name="consolidationDate" type="java.util.Date" ></property>
		<property name="available" type="double"></property>
		<property name="accrued" type="double"></property>
        
    </class>
</hibernate-mapping>