		<id name="id" type="java.lang.Integer">
            <column name="BT_ID" />
            <generator class="identity" />
        </id>
        
		<discriminator column="clase" type="string" />

		<subclass name="ar.edu.unq.desapp.grupoa.model.Bills.BillA"
			discriminator-value="BillA">
		</subclass>

		<subclass name="ar.edu.unq.desapp.grupoa.model.Bills.BillB"
			discriminator-value="BillB">
		</subclass>
		
		<subclass name="ar.edu.unq.desapp.grupoa.model.Bills.BillC"
			discriminator-value="BillC">
		</subclass>
		
		<subclass name="ar.edu.unq.desapp.grupoa.model.Bills.BillD"
			discriminator-value="BillD">
		</subclass>
		
		<subclass name="ar.edu.unq.desapp.grupoa.model.Bills.BillX"
			discriminator-value="BillX">
		</subclass>
	</class>
</hibernate-mapping>